import java.io.*;
import java.util.*;
import java.text.*;
public class Question_13
{
	public static void main(String[] args) {
		SimpleDateFormat formatDate = new SimpleDateFormat();//initialize SimpleDateFormat class
        Date date = new Date(); // initialize Date class
        formatDate.setTimeZone(TimeZone.getTimeZone("IST"));// converting to IST or format the Date as IST
        System.out.println(formatDate.format(date)); // print formatted date and time
	}
}